  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/node_modules/.bin/alanode",
      "args": [
        "example/multiple"
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
    ],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "args": [
        "test/spec",
        "-t", "9999999",
        "-a",
        "-w",
      ],
      "console": "integratedTerminal",
      "skipFiles": [
          "<node_internals>/**"
      ],
    }
  ]
}
